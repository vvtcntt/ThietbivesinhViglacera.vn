@{
    ViewBag.Title = "Xem đơn hàng";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
<link href="~/Content/Display/Style/Order.css" rel="stylesheet" />
<link href="~/Content/Display/Style/Order_Rs.css" rel="stylesheet" />



@Html.Raw(ViewBag.Status)
@model Viglacera.Models.tblOrder
<div class="container-fluid">
    <div style="width: 100%; margin: 0 auto;">
        <div id="Breadcrumb" class="Block Breadcrumb ui-widget-content ui-corner-top ui-corner-bottom">
            <ul>
                <li class='SecondLast'><a href=""><i class="icon-home" style="font-size:14px;"></i> Trang chủ</a></li>
                <li class='Last'><span>Danh sách đơn hàng</span></li>
            </ul>
        </div>
        <div style="clear: both;"></div>
        <div id="cph_Main_ContentPane">
            <div class="widget">
                <div class="widget-title">
                    <h4>Hạng mục đơn hàng</h4>

                    <div class="ui-widget-content ui-corner-top ui-corner-bottom">
                        <div id="toolbox">
                            <div style="float:right;" class="toolbox-content">
                                <div id="msg" class="msg" style="float:left">
                                    <div class="alert alert-info" style="padding:0px" id="update-message">
                                    </div>
                                </div>
                                <table class="toolbar">
                                    <tr>
                                        <td align="center">
                                            <a href="/Orderad/Index" title="" class="toolbar btn btn-info">Xác nhận đơn hàng</a>
                                         </td>
                                        <td align="center">
                                            <a href="/Orderad/Delete?id=@Model.id" title="" class="toolbar btn btn-info"><i class="icon-trash"></i>&nbsp;Xóa đơn hàng</a>

                                         </td>
                                        <td align="center">
                                            <a id="cph_Main_ctl00_toolbox_rptAction_lbtAction_0" title="Trợ giúp"  style="color:#FFF !important" class="toolbar btn btn-info" href="">
                                                <i class="icon-question-sign"></i>&nbsp;
                                                Trợ giúp
                                            </a>
                                        </td>
                                    </tr>
                                </table>
                            </div>
                        </div>
                    </div>
                    <div id="hiddenToolBarScroll" class="scrollBox hidden">
                        <h4>
                            <i class="icon-list-alt"></i>&nbsp;Hạng mục tài khoản thanh toán
                        </h4>
                        <div class="FloatMenuBar">

                            <div class="ui-widget-content ui-corner-top ui-corner-bottom">
                                <div id="toolbox">
                                    <div style="float:right;" class="toolbox-content">
                                        <div id="msg" class="msg" style="float:left">
                                            <div class="alert alert-info" id="update-message">
                                            </div>
                                        </div>
                                        <table class="toolbar">
                                            <tr>



                                                <td align="center">
                                                    <a href="/Orderad/Index" title="" class="toolbar btn btn-info">Xác nhận đơn hàng</a>
                                                </td>
                                                <td align="center">
                                                    <a href="/Orderad/Delete?id=@Model.id" title="" class="toolbar btn btn-info"><i class="icon-trash"></i>&nbsp;Xóa đơn hàng</a>

                                                </td>
                                            </tr>
                                        </table>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="widget-body">

                    <div id="Main">
                        <div id="Order" style="padding:0px">
                            <div id="ListInfoOrder">
                                <div class="nVar_Order">
                                    <span>Thông tin khách hàng</span>
                                </div>
                                <div class="Content_List">
                                    <div class="RowOrd">
                                        <div class="Left_RowOrd">
                                            <span>Họ và tên</span>
                                        </div>
                                        <div class="Right_RowOrd">
                                            @Html.TextBoxFor(p => p.Name, new { @class = "txtName", @autofocus = "autofocus", @required = "required", @disabled = "disabled", @placeholder = "Nhập họ tên" })

                                        </div>


                                    </div>
                                    <div class="RowOrd">
                                        <div class="Left_RowOrd">
                                            <span>Điện thoại</span>
                                        </div>
                                        <div class="Right_RowOrd">
                                            @Html.TextBoxFor(p => p.Mobile, new { @type = "tel", @class = "txtMobile", @autofocus = "autofocus", @disabled = "disabled", @required = "required", @placeholder = "Điện thoại" })
                                            @Html.TextBoxFor(p => p.Mobiles, new { @type = "tel", @class = "txtMobile", @autofocus = "autofocus", @disabled = "disabled", @placeholder = "Số khác" })


                                        </div>


                                    </div>
                                    <div class="RowOrd">
                                        <div class="Left_RowOrd">
                                            <span>Email</span>
                                        </div>
                                        <div class="Right_RowOrd">
                                            @Html.TextBoxFor(p => p.Email, new { @type = "email", @class = "txtEmail", @required = "required", @disabled = "disabled", @autofocus = "autofocus", @placeholder = "Nhập Email" })
                                        </div>


                                    </div>
                                    <div class="RowOrd">
                                        <div class="Left_RowOrd">
                                            <span>Địa chỉ</span>
                                        </div>
                                        <div class="Right_RowOrd">
                                            @Html.TextAreaFor(p => p.Address, new { @class = "txtAddress", @autofocus = "autofocus", @disabled = "disabled", @required = "required", @placeholder = "Nhập địa chỉ" })


                                        </div>


                                    </div>
                                    <div class="VAT">

                                        @if (Model.NameCP != "")
                                        {
                                            <div class="row_Vat">
                                                <input type="checkbox" id="chkvat" checked="checked" name="chkvat" class="chkvat" />
                                                <span>Yêu cầu INAX xuất hóa đơn đỏ <span>(VAT)</span> cho công ty hoặc tổ chức</span>

                                            </div>
                                            <div class="ContentVAT" style="display:block">
                                                <div class="RowOrd">
                                                    <div class="Left_RowOrd">
                                                        <span>Tên công ty</span>
                                                    </div>
                                                    <div class="Right_RowOrd">
                                                        @Html.TextBoxFor(p => p.NameCP, new { @class = "txtName", @autofocus = "autofocus", @disabled = "disabled", @placeholder = "Nhập tên công ty" })


                                                    </div>
                                                </div>
                                                <div class="RowOrd">
                                                    <div class="Left_RowOrd">
                                                        <span>Địa chỉ</span>
                                                    </div>
                                                    <div class="Right_RowOrd">
                                                        @Html.TextAreaFor(p => p.AddressCP, new { @class = "txtAddress", @disabled = "disabled", @autofocus = "autofocus", @placeholder = "Nhập địa chỉ công ty" })


                                                    </div>
                                                </div>
                                                <div class="RowOrd">
                                                    <div class="Left_RowOrd">

                                                        <span>Mã số thuế</span>
                                                    </div>
                                                    <div class="Right_RowOrd">
                                                        @Html.TextBoxFor(p => p.MST, new { @class = "txtMST", @disabled = "disabled", @autofocus = "autofocus", @placeholder = "Nhập mã số thuế" })


                                                    </div>
                                                </div>
                                                <div class="Clear"></div>
                                                <span class="Note">(*) Khách hàng vui lòng liên hệ hotline để biết đơn hàng có được hỗ trợ VAT hay không !</span>
                                            </div>
                                        }
                                    </div>
                                    <div class="Clear"></div>
                                    <div class="Nhanhang">

                                        @if (Model.Name1 != "")
                                        {
                                            <div class="row_nhanhang">
                                                <input type="checkbox" id="chknhanhang" checked="checked" name="chknhanhang" class="chknhanhang" />
                                                <span>Nhận hàng tại địa chỉ khác</span>
                                            </div>
                                            <div class="Content_Nhanhang" style="display:block">
                                                <div class="RowOrd">
                                                    <div class="Left_RowOrd">
                                                        <span>Họ và tên</span>
                                                    </div>
                                                    <div class="Right_RowOrd">
                                                        @Html.TextBoxFor(p => p.Name1, new { @class = "txtName", @disabled = "disabled", @autofocus = "autofocus", @placeholder = "Nhập họ tên" })

                                                    </div>


                                                </div>
                                                <div class="RowOrd">
                                                    <div class="Left_RowOrd">
                                                        <span>Điện thoại</span>
                                                    </div>
                                                    <div class="Right_RowOrd">
                                                        @Html.TextBoxFor(p => p.Mobile1, new { @type = "tel", @class = "txtMobile", @disabled = "disabled", @autofocus = "autofocus", @placeholder = "Điện thoại" })
                                                        @Html.TextBoxFor(p => p.Mobile1s, new { @type = "tel", @class = "txtMobile", @disabled = "disabled", @autofocus = "autofocus", @placeholder = "Số khác" })


                                                    </div>


                                                </div>
                                                <div class="RowOrd">
                                                    <div class="Left_RowOrd">
                                                        <span>Email</span>
                                                    </div>
                                                    <div class="Right_RowOrd">
                                                        @Html.TextBoxFor(p => p.Email1, new { @type = "email", @disabled = "disabled", @class = "txtEmail", @autofocus = "autofocus", @placeholder = "Nhập Email" })
                                                    </div>


                                                </div>
                                                <div class="RowOrd">
                                                    <div class="Left_RowOrd">
                                                        <span>Địa chỉ</span>
                                                    </div>
                                                    <div class="Right_RowOrd">
                                                        @Html.TextAreaFor(p => p.Address1, new { @class = "txtAddress", @disabled = "disabled", @autofocus = "autofocus", @placeholder = "Nhập địa chỉ" })


                                                    </div>


                                                </div>

                                            </div>
                                        }
                                    </div>
                                </div>
                                <div class="Clear"></div>

                                <a href="" class="timhieu" title="" target="_blank">>> Chính sách bảo mật thông tin</a>
                                <div class="nVar_Order">
                                    <span>Hình thức thanh toán</span>
                                </div>
                                <div class="Thanhtoan">
                                    <div class="row_thanhtoan">
                                        @if (Model.TypePay == 1)
                                        {
                                            <input type="radio" id="rdtt" name="rdtt" value="1" checked="checked" class="rdtttn" /><span>Giao hàng thu tiền tại nhà</span>

                                        }
                                        else
                                        {
                                            <input type="radio" id="rdtt" name="rdtt" value="1" class="rdtttn" /><span>Giao hàng thu tiền tại nhà</span>

                                        }
                                    </div>
                                    <div class="row_thanhtoan">
                                        @if (Model.TypePay == 2)
                                        {
                                            <input type="radio" id="rdtt" name="rdtt" value="2" checked="checked" class="rdttct" /><span>Nhận hàng và thanh toán tại INAX</span>
                                        }
                                        else
                                        {
                                            <input type="radio" id="rdtt" name="rdtt" value="2" class="rdttct" /><span>Nhận hàng và thanh toán tại INAX</span>

                                        }
                                    </div>
                                    <div class="row_thanhtoan">
                                        @if (Model.TypePay == 3)
                                        {
                                            <input type="radio" id="rdtt" name="rdtt" value="3" class="rdttck" checked="checked" /><span>Chuyển khoản ATM & Ngân hàng</span>
                                        }
                                        else
                                        {                     <input type="radio" id="rdtt" name="rdtt" value="3" class="rdttck" /><span>Chuyển khoản ATM & Ngân hàng</span>
                                        }
                                    </div>
                                    <div class="row_thanhtoan">
                                        @if (Model.TypePay == 3)
                                        {
                                            <input type="radio" id="rdtt" name="rdtt" value="3" class="rdttvs" checked="checked" /><span>Thanh toán visa, mastercard</span>
                                        }
                                        else
                                        {
                                            <input type="radio" id="rdtt" name="rdtt" value="3" class="rdttvs" /><span>Thanh toán visa, mastercard</span>

                                        }
                                    </div>

                                    <div class="Clear"></div>

                                    <a href="" class="timhieu" title="" target="_blank">>> Chính sách thanh toán của INAX</a>

                                </div>
                                <div class="nVar_Order">
                                    <span>Thời gian chuyển hàng</span>
                                </div>
                                <div class="Vanchuyen">
                                    <div class="row_Vanchuyen">
                                        @if (Model.TypeTransport == 1)
                                        {
                                            <input type="radio" id="rdvc" name="rdvc" value="1" checked="checked" /><span>Bất kỳ giờ nào</span>
                                        }
                                        else
                                        {
                                            <input type="radio" id="rdvc" name="rdvc" value="1" /><span>Bất kỳ giờ nào</span>

                                        }
                                    </div>
                                    <div class="row_Vanchuyen">
                                        @if (Model.TypeTransport == 2)
                                        {
                                            <input type="radio" id="rdvc" name="rdvc" value="2" checked /><span>Trong giờ hàng chính</span>
                                        }
                                        else
                                        {
                                            <input type="radio" id="rdvc" name="rdvc" value="2" /><span>Trong giờ hàng chính</span>

                                        }

                                    </div>
                                    <div class="row_Vanchuyen">
                                        @if (Model.TypeTransport == 3)
                                        {
                                            <input type="radio" id="rdvc" name="rdvc" value="3" checked="checked" /><span>Ngoài giờ hàng chính</span>
                                        }
                                        else
                                        {
                                            <input type="radio" id="rdvc" name="rdvc" value="3" /><span>Ngoài giờ hàng chính</span>

                                        }
                                    </div>	                    <div class="Clear"></div>

                                    <a href="" class="timhieu" title="" target="_blank">>> Chính sách vận chuyển của INAX</a>

                                </div>
                                <div class="RowOrd1">
                                    <div class="Left_RowOrd">
                                        <span>Lời nhắn</span>
                                    </div>
                                    <div class="Right_RowOrd">
                                        @Html.TextAreaFor(p => p.Description, new { @class = "txtContent", @disabled = "disabled", @autofocus = "autofocus", @placeholder = "Nhập lời nhắn cho đơn hàng" })


                                    </div>


                                </div>
                                <div class="Clear"></div>
                             </div>
                            <div id="ListOrder">
                                <div class="nVar_Order">
                                    <span>Thông tin sản phẩm</span>
                                </div>
                                <div id="Content_ListOrder">
                                    @Html.Raw(ViewBag.chuoi)

                                </div>
                            </div>
                            <div>
                            </div>
                        </div>
                        <p>&nbsp;</p>
                    </div>
                     

                </div>
            </div>
        </div>

    </div>
</div>


<script type="text/javascript">
    $(".alert").hide();
    $(".alert1").show();
    </script>