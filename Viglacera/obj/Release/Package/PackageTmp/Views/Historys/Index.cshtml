@using PagedList.Mvc;
@model PagedList.IPagedList<Viglacera.Models.HistoryView>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

@using (Html.BeginForm("Command", "Historys", FormMethod.Post))
{
    <div id="Content_Detail">
        <div class="Nvar_Content_Detail">
            <div class="Left_Nvar">
                <img src="/Content/Admin/Icon/folder.png" alt="Danh mục sản phẩm" />History
            </div>

            <div class="Right_Nvar">
                <div id="msg" class="msg">
                    <div class="alert alert-info" id="update-message">
                        <span>@Html.Raw(ViewBag.result)</span>
                    </div>
                </div>
            </div>
        </div>


        <div class="/Content_Content_Detail">
            <div class="Nvar_1">
                 

                <div class="clo_2" style="width:55%">UserName</div>
                <div class="clo_3" style="width:20%">Task</div>
                <div class="clo_4" style="width:20%">Datetime</div>
     
            </div>
            @foreach (var item in Model)
            {
                <div class="Nvar_2">
                     

                    <div class="clo_2" style="width: 55%"> @item.UserName  </div>
                    <div class="clo_3" style="width:20%">
                        @item.Task

                    </div>
                    <div class="clo_4" style="width:20%">
                        @item.DateCreate
                    </div>
                     
                </div>

            }
             

        </div>



        @Html.PagedListPager(Model, page => Url.Action("Index", new { page = page, sortOrder = ViewBag.CurrentSortOrder, sortName = ViewBag.CurrentSortName, sortLevel = ViewBag.CurrentSortLevel }), (PagedListRenderOptions)ViewBag.ship)

    </div>
}
