@model Viglacera.Models.tblPartner

@{
    ViewBag.Title = "Create"; Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}



@using (Html.BeginForm())
{
    @Html.ValidationSummary(true)


    <div class="Nvar_Content_Detail">
        <div class="Left_Nvar">
            <img src="/Content/Admin/Icon/folder.png" alt="Danh mục sản phẩm" />Edit Partner
        </div>

    </div>
    <div class="Content_Content_Detail">

        <div class="tr_1">
            <div class="left_tr1">
                <span>Name :</span>
            </div>
            <div class="Right_tr1">
                @Html.TextBoxFor(p => p.Name, new { @class = "Leght_2" })

            </div>
        </div>
        <div class="tr_1">
            <div class="left_tr1">
                <span>Address :</span>
            </div>
            <div class="Right_tr1">
                @Html.TextBoxFor(p => p.Address, new { @class = "Leght_2" })

            </div>
        </div>
        <div class="tr_1">
            <div class="left_tr1">
                <span>Mobile :</span>
            </div>
            <div class="Right_tr1">
                @Html.TextBoxFor(p => p.Mobile, new { @class = "Leght_2" })

            </div>
        </div>
        <div class="tr_1">
            <div class="left_tr1">
                <span>Hotline :</span>
            </div>
            <div class="Right_tr1">
                @Html.TextBoxFor(p => p.Hotline, new { @class = "Leght_2" })

            </div>
        </div>
        <div class="tr_1">
            <div class="left_tr1">
                <span>Email :</span>
            </div>
            <div class="Right_tr1">
                @Html.TextBoxFor(p => p.Email, new { @class = "Leght_2" })

            </div>
        </div>
        <div class="tr_1">
            <div class="left_tr1">
                <span>Ord:</span>
            </div>
            <div class="Right_tr1">
                @Html.TextBoxFor(p => p.Ord, new { @class = "txtWarraty"  })

            </div>
        </div>
        <div class="tr_1">
            <div class="left_tr1">
                <span>Active :</span>
            </div>
            <div class="Right_tr1">
                @Html.RadioButtonFor(p => p.Active, "true", new { @id = "Active" }) : Yes
                @Html.RadioButtonFor(p => p.Active, "false", new { @id = "Active" }): No
            </div>
        </div>


        <div class="tr_1">
            <div class="left_tr1">
            </div>
            <div class="Right_tr1">
                <button type="submit" id="btnAdd" name="btnAdd" class="btnAdd">Save</button>

            </div>
        </div>
    </div>
}
