    <meta http-equiv="content-language" content="vi" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    @Styles.Render("~/Content/Display/Css/Style")
    <link href="~/Content/Display/Css/update/call.css" rel="stylesheet" />
    <link href="~/Content/Display/Css/update/owl.carousel.min.css" rel="stylesheet" />
    <link href="~/Content/Display/Css/update/owl.theme.default.min.css" rel="stylesheet" />
    <link href="~/Content/Display/Css/update/Popup_ud.css" rel="stylesheet" />
    <link href="~/Content/Display/Css/update/product.css" rel="stylesheet" />
    <link href="~/Content/Display/Css/update/sale.css" rel="stylesheet" />
    <link href="~/Content/Display/Css/update/times.css" rel="stylesheet" />
    <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>

    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
            m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-27277318-14', 'auto');
        ga('send', 'pageview');
    </script>
    <script src="~/Scripts/jquery-ui.js"></script>
    <script src="~/Content/Display/Script/jquery-1.9.0.min.js"></script>
</head>
<body>
    @{Html.RenderAction("PartialTop", "Default");}
    <div class="Clear"></div>
    @{Html.RenderAction("PartialBanner", "Default");}
    <div class="Clear"></div>@{Html.RenderAction("PartialMenuMobile", "Default");}
    @{Html.RenderAction("PartialMenu", "Default");} <div id="Content">

        @RenderBody()
        <div class="Clear"></div>
    </div>
    <div class="Clear"></div>
    <div id="Services">
        <div class="sv0"></div>
        <div class="sv1">  </div>
        <div class="sv2"></div>
        <div class="sv3"></div>
        <div class="sv4"></div>
        <div class="Clear"></div>
    </div>
    <div class="Clear"></div>
    <div id="Footer">
        @{Html.RenderAction("PartialFootter", "Default");}
        <div class="Clear"></div>
        <div id="Bottom">
            <div id="Contentss">
                <span>
                    Copyright 2015  SEABIG VIET NAM . Allright reserved
                </span>
                <span id="connectedUsers"></span>


                <div id="Right">
                    <a href="" class="Face"></a>
                    <a href="" class="Google"></a>
                    <a href="" class="Youtube"></a>
                </div>
            </div>

        </div>
        <div class="Clear"></div>

        <script src="~/Content/Display/Script/jquery.nivo.slider.js"></script>
        <script src="~/Content/Display/Script/jquery.nivo.slider.pack.js"></script>
        <script src="~/Scripts/plusone.js"></script>
        <script type="text/javascript">
            $(window).load(function () {
                $('#slider').nivoSlider();
            });
        </script>
        <script>
            window.fbAsyncInit = function () {
                FB.init({
                    appId: '1600906160151458',
                    xfbml: true,
                    version: 'v2.3'
                });
            };

            (function (d, s, id) {
                var js, fjs = d.getElementsByTagName(s)[0];
                if (d.getElementById(id)) { return; }
                js = d.createElement(s); js.id = id;
                js.src = "//connect.facebook.net/en_US/sdk.js";
                fjs.parentNode.insertBefore(js, fjs);
            }(document, 'script', 'facebook-jssdk'));
        </script>
        <script src="~/Scripts/jquery.mmenu.min.all.js"></script>
        <script type="text/javascript">
            $(function () {
                $('nav#menu').mmenu({
                    extensions: ['effect-slide', 'pageshadow'],
                    header: true,
                    searchfield: true,
                    counters: true,
                    footer: {
                        add: true,

                    }
                });
            });

            </script>
            <script src="~/Content/Display/Script/update/jquery.popupMiendatweb.min.js"></script>
        <script src="~/Content/Display/Script/update/owl.carousel.min.js"></script>
        <script>
            $(document).ready(function () {
                $('.owl-carousel').owlCarousel({
                    loop: true,
                    margin: 10,
                    autoplay: true,
                    autoplayTimeout: 5000,
                    autoplayHoverPause: true,
                    responsiveClass: true,
                    responsive: {
                        0: {
                            items: 1,
                            nav: true
                        },
                        600: {
                            items: 2,
                            nav: false
                        },
                        1000: {
                            items: 3,
                            nav: true,
                            loop: false,
                            margin: 20
                        }
                    }
                })
            })
        </script>
        <script type="text/javascript" language="JavaScript">
                            $(function () {
                        /* khởi tạo popup */
                        $('input[rel*=miendatwebPopup]').showPopup({
                            top: 200, //khoảng cách popup cách so với phía trên
                            closeButton: ".close_popup", //khai báo nút close cho popup
                            scroll: false, //cho phép scroll khi mở popup, mặc định là không cho phép
                            onClose: function () {
                                //sự kiện cho phép gọi sau khi đóng popup, cho phép chúng ta gọi 1 số sự kiện khi đóng popup, bạn có thể để null ở đây
                            }
                        });
                    $('a[rel*=miendatwebPopup]').showPopup({
                        top: 200, //khoảng cách popup cách so với phía trên
                closeButton: ".close_popup", //khai báo nút close cho popup
                scroll: false, //cho phép scroll khi mở popup, mặc định là không cho phép
                                    onClose: function () {
                        //sự kiện cho phép gọi sau khi đóng popup, cho phép chúng ta gọi 1 số sự kiện khi đóng popup, bạn có thể để null ở đây
                    }
                    });
                });
                            function close_popup() {
                        $("#popup_content").css("display", "none");
                    $("#lean_overlay").css("display", "none");

                }
        </script>
    </div>
</body>
</html>
