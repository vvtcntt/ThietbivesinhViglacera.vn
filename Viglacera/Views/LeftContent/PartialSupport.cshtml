@model IEnumerable<Viglacera.Models.tblSupport>
<div class="timeWorks">
    <span class="tm1">Thời gian làm việc : <strong>8H00 - 21H15</strong> </span>
    <span class="tm2">Làm việc tất cả các ngày trong tuần</span>
    <span class="tm3">
        Khách hàng mua hàng từ <strong>18H00 - 21H15</strong> cần đặt lịch trước qua số hotline : <strong>1900 63 66 95</strong>
    </span>
</div>
<div class="Box_Hotline">
    <a href="#" title="#">Hotline : <span>@Html.Raw(ViewBag.Hotline)</span></a>
</div>
<div class="Box_Hotline">
    <a href="#" title="#">Tổng đài : <span>@Html.Raw(ViewBag.HotlineOUT)</span></a>
</div>
<div class="Box_Hotline">
    <a href="#" title="#">Bảo hành : <span>@Html.Raw(ViewBag.MobileOUT)</span></a>
</div>
<div id="Supports">
    <div id="Top_Support"> <span>Hỗ trợ trực tuyến</span></div>
    <div id="Bottom_Support">
        @foreach (var item in Model)
        {
            <div class="Tear_Supports">
                <div class="Left_Tear_Support">
                    <span class="htv1">@item.Mission:</span><span class="htv2">@item.Name</span>
                </div>
                <div class="Right_Tear_Support">
                    <div class="topTearSupport">
                        <a href="tel:@item.Mobile" title="@item.Name"><img src="/Content/Display/iCon/logo_zalo.png" alt="@item.Name"></a>
                        <a href="tel:@item.Mobile" title="@item.Name"><img src="/Content/Display/iCon/Viber_logo.png" alt="@item.Name"></a>
                    </div>
                    <div class="bottomTearSupport"><span>@item.Mobile</span></div>
                </div>
            </div>
        }
    </div>
    <div class="Clear"></div>
</div>
<div id="Box_Link">
    <div class="Top_Link">
        <span>Liên kết website</span>
    </div>
    <div class="Bottom_Link">
        @Html.Raw(ViewBag.url)
    </div>
</div>
<div class="GoogleAdsemse">
    <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>

    <ins class="adsbygoogle"
         style="display:block"
         data-ad-client="ca-pub-8738210846913491"
         data-ad-slot="5698818364"
         data-ad-format="auto"></ins>
    <script>
        (adsbygoogle = window.adsbygoogle || []).push({});
    </script>
</div>