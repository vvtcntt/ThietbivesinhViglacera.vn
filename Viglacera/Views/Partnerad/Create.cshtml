                                <!-- Messange -->
                                @Html.Raw(ViewBag.thongbao)

                                <!-- End Messange -->
                            </div>
                            <table class="admintable" style="width: 100%;">
                                <tr>
                                    <td class="key">
                                        Tên liên kết : <span class="Required">*</span>
                                    </td>
                                    <td>
                                        @Html.TextBoxFor(p => p.Name, new { @class = "Name_ad txtName", @autofocus = "autofocus", @required = "required" })


                                        <span class="tooltip-help">
                                            <span class="tooltipContent">
                                                <p class="tooltiptitle">
                                                    Tên liên kết
                                                </p>
                                                <p class="tooltipmessage">
                                                    Tên liên kết
                                                </p>
                                            </span>
                                        </span>
                                        <span class="Aler_ttxtName" style="color: #F00"></span>
                                    </td>
                                </tr>
                                <tr>
                                    <td class="key">
                                        Tên đối tác : <span class="Required">*</span>
                                    </td>
                                    <td>
                                        @Html.TextBoxFor(p => p.Name, new { @class = "Name_ad txtName", @autofocus = "autofocus", @required = "required" })


                                        <span class="tooltip-help">
                                            <span class="tooltipContent">
                                                <p class="tooltiptitle">
                                                   Tên đối tác
                                                </p>
                                                <p class="tooltipmessage">
                                                    Tên đối tác                        
                                                                        </p>
                                            </span>
                                        </span>
                                        <span class="Aler_ttxtName" style="color: #F00"></span>
                                    </td>
                                </tr>
                                <tr>
                                    <td class="key">
                                        Địa chỉ : <span class="Required">*</span>
                                    </td>
                                    <td>
                                        @Html.TextBoxFor(p => p.Address, new { @class = "Name_ad txtName", @autofocus = "autofocus"})


                                        <span class="tooltip-help">
                                            <span class="tooltipContent">
                                                <p class="tooltiptitle">
                                                    Địa chỉ
                                                </p>
                                                <p class="tooltipmessage">
                                                   Địa chỉ
                                                </p>
                                            </span>
                                        </span>
                                        <span class="Aler_ttxtName" style="color: #F00"></span>
                                    </td>
                                </tr>
                                <tr>
                                    <td class="key">
                                        Điện thoại : <span class="Required">*</span>
                                    </td>
                                    <td>
                                        @Html.TextBoxFor(p => p.Mobile, new { @class = "Name_ad txtName", @autofocus = "autofocus" })


                                        <span class="tooltip-help">
                                            <span class="tooltipContent">
                                                <p class="tooltiptitle">
                                                    Điện thoại
                                                </p>
                                                <p class="tooltipmessage">
                                                    Điện thoại
                                                </p>
                                            </span>
                                        </span>
                                        <span class="Aler_ttxtName" style="color: #F00"></span>
                                    </td>
                                </tr>
                                <tr>
                                    <td class="key">
                                       Di động : <span class="Required">*</span>
                                    </td>
                                    <td>
                                        @Html.TextBoxFor(p => p.Hotline, new { @class = "Name_ad txtName", @autofocus = "autofocus" })


                                        <span class="tooltip-help">
                                            <span class="tooltipContent">
                                                <p class="tooltiptitle">
                                                   Di động
                                                </p>
                                                <p class="tooltipmessage">
                                                    Di động
                                                </p>
                                            </span>
                                        </span>
                                        <span class="Aler_ttxtName" style="color: #F00"></span>
                                    </td>
                                </tr>
                                <tr>
                                    <td class="key">
                                        Email : <span class="Required">*</span>
                                    </td>
                                    <td>
                                        @Html.TextBoxFor(p => p.Email, new { @class = "Name_ad txtName", @autofocus = "autofocus" })


                                        <span class="tooltip-help">
                                            <span class="tooltipContent">
                                                <p class="tooltiptitle">
                                                    Email
                                                </p>
                                                <p class="tooltipmessage">
                                                    Email
                                                </p>
                                            </span>
                                        </span>
                                        <span class="Aler_ttxtName" style="color: #F00"></span>
                                    </td>
                                </tr>
                                <tr>
                                    <td class="key">
                                        Thứ tự
                                    </td>
                                    <td>
                                        @Html.TextBoxFor(p => p.Ord, new { @class = "Ord_ad", @Value = @Html.Raw(ViewBag.Ord) })


                                        <span class="tooltip-help">
                                            <span class="tooltipContent">
                                                <p class="tooltiptitle">
                                                    Thứ tự hiển thị
                                                </p>
                                                <p class="tooltipmessage">
                                                    Thứ tự hiển thị
                                                </p>
                                            </span>
                                        </span>
                                    </td>
                                </tr>
                                 

                                <tr>
                                    <td class="key">
                                        Kích hoạt
                                    </td>
                                    <td>
                                        @{bool Active = true;}
                                        @Html.CheckBoxFor(model => Active)

                                        <span class="tooltip-help">
                                            <span class="tooltipContent">
                                                <p class="tooltiptitle">
                                                    Hiển thị
                                                </p>
                                                <p class="tooltipmessage">
                                                    bạn có kích hoạt mục này này chạy hay không
                                                </p>
                                            </span>
                                        </span>
                                    </td>
                                </tr>
                            </table>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
                                            }
